{
  "name": "@uni-component/mpx",
  "version": "0.4.0",
  "description": "Unified component for Mpx",
  "main": "index.js",
  "module": "dist/uni-component-mpx.mjs",
  "unpkg": "dist/uni-component-mpx.iife.prod.js",
  "jsdelivr": "dist/uni-component-mpx.iife.prod.js",
  "types": "dist/uni-component-mpx.d.ts",
  "exports": {
    ".": {
      "browser": "./dist/uni-component-mpx.esm-browser.js",
      "node": {
        "import": {
          "production": "./dist/uni-component-mpx.prod.cjs",
          "development": "./dist/uni-component-mpx.mjs",
          "default": "./dist/uni-component-mpx.mjs"
        },
        "require": {
          "production": "./dist/uni-component-mpx.prod.cjs",
          "development": "./dist/uni-component-mpx.cjs",
          "default": "./index.js"
        }
      },
      "import": "./dist/uni-component-mpx.mjs"
    },
    "./package.json": "./package.json",
    "./dist/*": "./dist/*"
  },
  "sideEffects": false,
  "buildOptions": {
    "filename": "uni-component-mpx",
    "name": "UniComponentMpx"
  },
  "keywords": [
    "@uni-component/mpx",
    "component",
    "unicomponent",
    "uni-component",
    "un",
    "uni"
  ],
  "author": "dolymood@gmail.com",
  "files": [
    "dist/*.js",
    "dist/*.mjs",
    "dist/*.cjs",
    "dist/uni-component-mpx.d.ts",
    "index.js",
    "LICENSE",
    "README.md"
  ],
  "license": "MIT",
  "devDependencies": {
    "@mpxjs/core": "^2.6.110",
    "@uni-component/core": "*",
    "@uni-store/core": "^0.3.3"
  },
  "peerDependencies": {
    "@mpxjs/core": "^2.6.110",
    "@uni-component/core": "0.4.0",
    "@uni-store/core": ">=0.3.3"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dolymood/uni-component.git",
    "directory": "packages/mpx"
  },
  "bugs": {
    "url": "https://github.com/dolymood/uni-component/issues"
  },
  "homepage": "https://github.com/dolymood/uni-component#readme"
}
