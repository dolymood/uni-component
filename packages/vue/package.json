{
  "name": "@uni-component/vue",
  "version": "0.6.0",
  "description": "Unified component for Vue",
  "main": "index.js",
  "module": "dist/uni-component-vue.mjs",
  "unpkg": "dist/uni-component-vue.iife.prod.js",
  "jsdelivr": "dist/uni-component-vue.iife.prod.js",
  "types": "dist/uni-component-vue.d.ts",
  "exports": {
    ".": {
      "browser": "./dist/uni-component-vue.esm-browser.js",
      "node": {
        "import": {
          "production": "./dist/uni-component-vue.prod.cjs",
          "development": "./dist/uni-component-vue.mjs",
          "default": "./dist/uni-component-vue.mjs"
        },
        "require": {
          "production": "./dist/uni-component-vue.prod.cjs",
          "development": "./dist/uni-component-vue.cjs",
          "default": "./index.js"
        }
      },
      "import": "./dist/uni-component-vue.mjs"
    },
    "./package.json": "./package.json",
    "./dist/*": "./dist/*"
  },
  "sideEffects": false,
  "buildOptions": {
    "filename": "uni-component-vue",
    "name": "UniComponentVue"
  },
  "keywords": [
    "@uni-component/vue",
    "component",
    "unicomponent",
    "uni-component",
    "un",
    "uni"
  ],
  "author": "dolymood@gmail.com",
  "files": [
    "dist/*.js",
    "dist/*.mjs",
    "dist/*.cjs",
    "dist/uni-component-vue.d.ts",
    "index.js",
    "LICENSE",
    "README.md"
  ],
  "license": "MIT",
  "devDependencies": {
    "@testing-library/vue": "^6.5.0",
    "@uni-component/core": "*",
    "@uni-component/example": "*",
    "@uni-store/core": "^0.3.3",
    "@vue/compiler-sfc": "3",
    "@vue/runtime-core": "^3.2.24",
    "@vue/runtime-dom": "^3.2.24",
    "vue": "^3.2.24"
  },
  "peerDependencies": {
    "@uni-component/core": "0.6.0",
    "@uni-store/core": ">=0.3.3",
    "vue": "^3"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dolymood/uni-component.git",
    "directory": "packages/vue"
  },
  "bugs": {
    "url": "https://github.com/dolymood/uni-component/issues"
  },
  "homepage": "https://github.com/dolymood/uni-component#readme"
}
